{
    "version": 2,
    "name": "google-maps-lead-scraper",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "server.js"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "public/$1"
        },
        {
            "source": "/",
            "destination": "public/index.html"
        }
    ]
}